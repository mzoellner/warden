{
  "name": "repository-warden",
  "version": "3.2.0",
  "description": "Print out warden information in your repository",
  "bin": {
    "warden": "./dist/index.js"
  },
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorisCoppieters/warden.git"
  },
  "author": "Joris Coppieters",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JorisCoppieters/warden/issues"
  },
  "homepage": "https://github.com/JorisCoppieters/warden#readme",
  "dependencies": {
    "color-print": "^1.0.1",
    "commander": "^2.17.1",
    "fs-process": "^1.0.0",
    "inquirer": "7.0.0",
    "jest-changed-files": "^23.0.1",
    "minimist": "^1.2.0",
    "modifiled": "^0.0.6"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.0.3",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "ts-node-dev": "^1.0.0-pre.21",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "ts-node-dev --respawn ./src/index.ts",
    "watch": "tsc --watch",
    "prepublish": "tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
